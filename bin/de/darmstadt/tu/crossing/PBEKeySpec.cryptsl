SPEC javax.crypto.spec.PBEKeySpec
OBJECTS 
	char[] pw;
	byte[] salt;
	int it;
	int keylength; 
	
EVENTS
	c1: PBEKeySpec(pw);
	c2: PBEKeySpec(pw, salt, it);
	c3: PBEKeySpec(pw, salt, it, keylength);
	
	Cons := c1 | c2 | c3;
	
	cP: clearPassword();
	
ORDER
 	Cons, cP*
CONSTRAINTS
	it >= 1000;

REQUIRES
	random[salt];	
ENSURES
	 keyspec[this, keylength] after c1;
NEGATES
	 keyspec[this, _] after cP;